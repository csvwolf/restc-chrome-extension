{
  "manifest_version": 2,
  "name": "restc",
  "version": "1.0.0",
  "background": {
    "scripts": ["./src/index.js"],
    "presistent": false
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "./src/settings.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://shadow.elemecdn.com 'sha256-+J1p1MHGYJQoGcnX1Ld/EeRzTw7bRVhaXqKjj2FH1iI='; object-src 'self'"
}
