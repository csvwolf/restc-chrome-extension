{
  "manifest_version": 2,
  "name": "restc",
  "version": "1.0.0",
  "background": {
    "scripts": ["index.js"],
    "presistent": false
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://shadow.elemecdn.com; object-src 'self'"
}
